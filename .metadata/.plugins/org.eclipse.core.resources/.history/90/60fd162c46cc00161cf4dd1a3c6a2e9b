package com.Assignment;
import java.util.*;
public class Main {
	public static void main(String args[]) {
		// Write your code here
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int arr[]=new int[N];
        for(int i=0;i<N;i++)
        {
            arr[i]=sc.nextInt();
        }
        
	}
}
